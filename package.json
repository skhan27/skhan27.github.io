{
  "name": "portfolio-site",
  "version": "1.0.0",
  "description": "personal portfolio site",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 version\" css/style.comp.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skhan27/skhan27.github.io.git"
  },
  "author": "Saim Khan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/skhan27/skhan27.github.io/issues"
  },
  "homepage": "https://github.com/skhan27/skhan27.github.io#readme",
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "concat": "^1.0.3",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "postcss-cli": "^4.1.1"
  }
}
